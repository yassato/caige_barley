@article{elger_plant_2002,
	title = {Plant palatability and disturbance level in aquatic habitats: an experimental approach using the snail {Lymnaea} stagnalis ({L}.)},
	volume = {47},
	issn = {1365-2427},
	shorttitle = {Plant palatability and disturbance level in aquatic habitats},
	url = {http://onlinelibrary.wiley.com/doi/10.1046/j.1365-2427.2002.00820.x/abstract},
	doi = {10.1046/j.1365-2427.2002.00820.x},
	abstract = {1. The palatability of aquatic macrophytes to the snail Lymnaea stagnalis was investigated in the laboratory. Eight species of macrophyte were selected from habitats that differed in either flood disturbance regime or nutrient status.2. In a non-choice test, single macrophyte species were offered to individual snails. The average amount of plant dry mass consumed per Lymnaea dry mass ranged from 3.6 ± 1.4 (±SE) to 63.6 ± 13.9 mg g–1 day–1 across plant species. In a choice test, all eight plant species were presented simultaneously to sets of five snails. The average total consumption was 66.1 ± 3.8 mg g–1 day–1 and the maximum average consumption for a single plant was 26.2 ± 3.6 mg g–1 day–1.3. In both tests, the amount consumed by snails differed significantly between the plant species. The species growing in undisturbed habitats were the least consumed. Habitat nutrient status was unrelated to plant palatability.4. These results suggest that macrophyte species growing in habitats that are rarely disturbed by floods allocate a greater proportion of their resources to resisting herbivory.},
	language = {en},
	number = {5},
	urldate = {2013-05-06},
	journal = {Freshwater Biology},
	author = {Elger, Arnaud and Barrat-Segretain, Marie-Hélène and Amoros, Claude},
	year = {2002},
	note = {Number: 5},
	keywords = {Herbivory, disturbance, aquatic macrophytes, life-history traits, nutrient status},
	pages = {931--940},
}

