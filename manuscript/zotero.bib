	urldate = {2021-02-19},
	journal = {Engineering Science and Technology, an International Journal},
	author = {Almryad, Ayad Saad and Kutucu, Hakan},
	month = feb,
	year = {2020},
	note = {Number: 1},
	pages = {189--195},
