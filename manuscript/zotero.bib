	number = {5},
	urldate = {2024-05-19},
	journal = {PLOS Genetics},
	author = {Evans, Luke M. and Arehart, Christopher H. and Grotzinger, Andrew D. and Mize, Travis J. and Brasher, Maizy S. and Stitzel, Jerry A. and Ehringer, Marissa A. and Hoeffer, Charles A.},
