}

@article{li_quantification_2023,
	title = {Quantification of the {Cumulative} {Shading} {Capacity} in a {Maize}–{Soybean} {Intercropping} {System} {Using} an {Unmanned} {Aerial} {Vehicle}},
	volume = {5},
