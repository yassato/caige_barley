	journal = {Plant Species Biology},
	author = {Yano, Shuichi},
	year = {1994},
	note = {Number: 3},
	keywords = {Herbivory, anti-herbivore defense, intrinsic food quality, plant phenology, Specialist},
	pages = {137--143},
}

