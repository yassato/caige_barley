	number = {8},
	urldate = {2024-09-18},
	journal = {G3 Genes{\textbar}Genomes{\textbar}Genetics},
	author = {Yamamoto, Eiji and Matsunaga, Hiroshi},
	editor = {Doerge, R},
	month = aug,
	year = {2021},
	pages = {jkab119},
	file = {Full Text:/Users/yasuhirosato/Zotero/storage/RB86W52K/Yamamoto and Matsunaga - 2021 - Exploring efficient linear mixed models to detect .pdf:application/pdf},
}

@article{devlin_genomic_1999,
