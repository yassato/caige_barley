	month = may,
	year = {2008},
	pages = {361--369},
	file = {Full Text:/Users/yasuhirosato/Zotero/storage/GM8URBX7/Gao et al. - 2008 - A multiple testing correction method for genetic a.pdf:application/pdf},
}

