@article{jerling_effects_1982,
	title = {Effects of selective grazing by cattle on the reproduction of {Plantago} maritima},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1600-0587.1982.tb01055.x/abstract},
	abstract = {The grazing areas of cattle in a Baltic sea-shore meadow changed during the summer. The effects of this on the demography of Plantago maritima was studied. This species is distributed over most parts of the area and suffers differently from grazing in different areas. It was found that the losses of flowers due to grazing, was severe and that the density peak of Plantago maritima coincides with the least grazing intensity. Flower-spikes established in different time-periods are disproportionally represented as seed-dispersers and this may be of importance in regeneration since early and late flowers bear seeds of different size.},
