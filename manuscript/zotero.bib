@article{watt_adaptive_1968,
	title = {Adaptive {Significance} of {Pigment} {Polymorphisms} in {Colias} {Butterflies}. {I}. {Variation} of {Melanin} {Pigment} in {Relation} to {Thermoregulation}},
	url = {http://www.jstor.org/stable/2406873},
	abstract = {Colias butterflies living in cold climates display great darkening of their hindwing undersides, but those in warm climates do not. Many populations of Colias have been studied to test the hypothesis that this darkening increases the efficiency of absorption of solar energy for heating, and thus promotes activity, hence reproductive success, in cold environments. Colias' body temperatures were monitored by the chronic implantation of minute thermistor probes, and the following results were obtained: 1) Below body temperatures of 34-35 C, Colias at rest orient to present their hindwing undersides perpendicular to the sun's rays, presenting maximum surface area for energy absorption. The thorax and abdomen lie directly beneath the area of greatest darkening on the wing. Above temperatures of 37-38 C, Colias at rest orient parallel to the sun's rays, presenting minimal surface area, and also seek shade. 2) All Colias show a similar voluntary activity range (VAR). The lower limit of this is 28-30 C; the upper limit lies above 40-42 C. Populations in cold habitats have not adapted by lowering the lower limit of the VAR, i.e. flying at lower temperatures. The optimum activity range (OAR) of Colias appears to be 35-38 C. 3) Colias display no mechanisms for facultative warming by metabolic or muscular heating, nor for facultative cooling by evaporation. 4) If a light and a dark Colias of the same mass and surface area are exposed side by side to solar radiation, the dark insect heats significantly faster, and attains higher steady-state body temperature, than does the light insect. These differences persist, both in direction and size, after death, and thus cannot be caused by any metabolic agency. Since light and dark forms lose heat at the same rates in cooling rate studies, the heating difference must result from greater solar energy absorption by dark insects. Reflectance spectrophotometry was performed on light and dark Colias wings at visible, near-infrared and infrared wavelengths. The energy absorption advantage of the dark forms over the light, as revealed by spectrophotometry, is wholly consistent with the heating rate advantage found in the thermistor studies. It is shown that, in cold alpine or northern habitats, dark Colias must achieve greater reproductive success than light Colias, since their solar heating advantage will lead to greater activity for feeding, mating, and oviposition. The reverse is true in warm lowland habitats, where light Colias will be less susceptible than dark ones to heat stress and resulting forced inactivity. Means for estimating the strength of these selective forces, and other forces which might act on underside color in Colias, are discussed.},
	urldate = {2011-07-11},
	author = {Watt, Ward B.},
	year = {1968},
	pages = {437--458},
}

@article{tsegai_effects_2013,
	title = {Effects of spatial distribution on plant associational defense against herbivory},
	volume = {14},
	issn = {1439-1791},
	url = {http://www.sciencedirect.com/science/article/pii/S1439179113001254},
	doi = {10.1016/j.baae.2013.09.007},
	abstract = {Several studies have shown that consumption of a focal plant by herbivores depends not only on its own defense traits but also on the characteristics of the neighboring plants. A number of studies have reported on plant associational defense in relation to neighboring plant palatability but the effect of the spatial distribution of the focal plant within patches of different neighboring plants has received less attention. We conducted a manipulative experiment to determine whether and how spatial distribution of focal plants affects the associational defense between plant species. In our experimental setup sheep encountered two patches varying in spatial distribution of the focal plant within patches (dispersed or clumped) and patch quality, good patch and bad patch, where the focal plant, Lathyrus quinquenervius, was neighbored to high- (Chloris virgata) or low-palatable (Kalimeris integrifolia) species, respectively. Results showed that, when focal plants were dispersed within both patches, the risk of attack was significantly lower for focal plants in the patches with low- than high-palatable neighbors, indicating associational defense. Alternatively, when focal plants were clumped within both patches, they were consumed in bad-patch as much as in good-patch plots, which indicates the absence of associational defense. However, if the focal plants have different spatial distributions in the two patches (dispersed in good-patch and clumped in bad-patch or vice versa), sheep foraging success for focal plants was greatly reduced in dispersed spatial pattern irrespective of the palatability of neighboring plants. Therefore, we concluded that spatial distribution is as important as traits of neighboring plants in predicting vulnerability of the focal plant to grazing by generalist herbivores. The outcome of plant associational defense for different types of neighborhood strongly depends on the magnitude of herbivore foraging selectivity between and within patches, which further depended on the contrasts between plant species or between patches.},
	number = {8},
	urldate = {2015-08-26},
	journal = {Basic and Applied Ecology},
	author = {Tsegai, Awet Tekeste and Wang, Ling and Wang, Deli and Huang, Yue and Lin, Haijiao and Li, Jing and Liu, Chen},
	year = {2013},
	note = {Number: 8},
	keywords = {Herbivory, spatial distribution, Associational plant defense, Selectivity, Neighboring plant contrast},
	pages = {680--686},
}

