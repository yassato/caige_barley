	journal = {Nature Communications},
	author = {Galvāo, Vinicius Costa and Fiorucci, Anne-Sophie and Trevisan, Martine and Franco-Zorilla, José Manuel and Goyal, Anupama and Schmid-Siegert, Emanuel and Solano, Roberto and Fankhauser, Christian},
	month = sep,
	year = {2019},
	pages = {4005},
	file = {Full Text:/Users/yasuhirosato/Zotero/storage/8MIVP8BB/Galvāo et al. - 2019 - PIF transcription factors link a neighbor threat c.pdf:application/pdf},
}

