@article{ferguson_host-plant_1991,
	title = {Host-{Plant} {Preference} of the {Asiatic} {Oak} {Weevil} (coleoptera, {Curculionidae})},
	volume = {20},
	issn = {0046-225X},
	abstract = {Natural oak regeneration is poor in Missouri's forests. The potential of the Asiatic oak weevil to affect Missouri's oak resource was addressed by determining the weevil's host plant preference for 10 common Missouri tree species. Host plant preference was assessed in two-choice feeding experiments and no-choice longevity and oviposition experiments. Changes in host plant preference because of previous feeding experience also was investigated. Black oak, red oak, white oak, and sugar maple were acceptable hosts for adult weevils because measurable feeding occurred on these species during two-choice preference tests. In 1985, significantly more leaf was consumed from each oak species than from sugar maple. Leaf consumption for sugar maple and black oak did not differ in 1986. Adults fed red oak and black oak in no-choice tests lived significantly longer and laid significantly more eggs than weevils fed white oak or sugar maple, indicating that red and black oaks were preferred hosts. Of the other species tested, ash, basswood, dogwood, elm, hickory, and sassafras were considered nonhosts because adults did not feed or oviposit when restricted to these tree species. Sugar maple and white oak were acceptable but not preferred hosts because longevity and egg production were reduced when weevils fed on these species. A feeding preference for white oak and sugar maple could not induced in adults by an earlier feeding experience on these hosts.},
	language = {English},
	number = {5},
	journal = {Environmental Entomology},
	author = {Ferguson, Cs and Linit, Mj and Krause, G.},
	year = {1991},
	note = {Number: 5},
	keywords = {herbivores, insects, Insecta, larvae, cyrtepistomus-castaneus, forest entomology, host preferences, leaf tannins, perspective},
	pages = {1427--1432},
}

