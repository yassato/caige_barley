@article{iddles_potential_2003,
	title = {The potential contribution of biomechanical properties to anti-herbivore defence in seedlings of six {Australian} rainforest trees},
	volume = {51},
	url = {http://www.publish.csiro.au/paper/BT02060},
	abstract = {Biomechanical properties of leaves, such as strength and toughness, may contribute to anti-herbivore defence by making it physically difficult or energetically costly for animals to eat them. We investigated leaf strength and toughness in seedlings of six rainforest trees from eastern Australia and their potential contribution to defence. Strength and toughness (work to fracture) were measured at the scale of the whole leaf and of different leaf parts. Resources for herbivores (water and nitrogen) and potential chemical defences (phenolics, alkaloids and cyanogenic glycosides) were also investigated.{\textbackslash}textlessp{\textbackslash}textgreaterLeaves of \textit{Nothofagus moorei} (F.Muell.) Krasser, \textit{Ceratopetalum apetalum} D.Don and \textit{Doryphora sassafras} Endl. were generally the toughest and strongest of those studied. \textit{Toona ciliata} M.Roemer showed high concentrations of nutritive resources and little investment in the defences investigated, consistent with its shorter leaf lifespan and higher growth rate. There were no significant correlations of mechanical properties with palatability, but the softest leaf (\textit{T. ciliata}) was associated with high levels of leaf damage in bioassay trials. There was also evidence that some leaves that are well-defended mechanically (involving carbon-rich cell wall) may invest less in competing carbon-based chemical defences, such as phenolics.{\textbackslash}textless/p{\textbackslash}textgreater},
	number = {1},
	urldate = {2013-07-30},
	journal = {Australian Journal of Botany},
	author = {Iddles, Tracey L. and Read, Jennifer and Sanson, Gordon D.},
	year = {2003},
	note = {Number: 1},
	pages = {119--128},
}

@article{ayala_competition_1971,
