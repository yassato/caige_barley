	urldate = {2024-01-25},
	institution = {Genetics},
	author = {Ferreira, Filipe M. and Chaves, Saulo F. S. and Bhering, Leonardo L. and Alves, Rodrigo S. and Takahashi, Elizabete K. and Resende, Marcos D. V. and Souza, João E. and Gezan, Salvador A. and Viana, José M. S. and Fernandes, Samuel B. and Dias, Kaio O. G.},
	month = apr,
	year = {2023},
	doi = {10.1101/2023.04.14.536978},
	file = {Submitted Version:/Users/yasuhirosato/Zotero/storage/LSU8TESE/Ferreira et al. - 2023 - A data-driven approach for enhancing forest produc.pdf:application/pdf},
}

