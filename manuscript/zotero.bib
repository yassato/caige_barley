	author = {Jhee, Edward M. and Boyd, Robert S. and Eubanks, Micky D.},
	year = {2005},
	note = {Number: 2},
	keywords = {Herbivory, aphid, folivore, hyperaccumulator, rhizovore, spidermite, spittlebug, Streptanthus polygaloides},
