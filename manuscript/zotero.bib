	file = {Full Text:/Users/yasuhirosato/Zotero/storage/4WTEU9LZ/Wallace et al. - 2018 - On the Road to Breeding 4.0 Unraveling the Good, .pdf:application/pdf},
}

@article{stockenhuber_uv_2024,
	title = {{UV} {RESISTANCE} {LOCUS} 8–{Mediated} {UV}-{B} {Response} {Is} {Required} {Alongside} {CRYPTOCHROME} 1 for {Plant} {Survival} in {Sunlight} under {Field} {Conditions}},
