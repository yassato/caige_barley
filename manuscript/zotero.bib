	pages = {35--48},
	file = {Full Text:/Users/yasuhirosato/Zotero/storage/N8BLDH7A/Stockenhuber et al. - 2024 - UV RESISTANCE LOCUS 8–Mediated UV-B Response Is Re.pdf:application/pdf},
}

