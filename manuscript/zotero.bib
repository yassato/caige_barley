@article{zhang_gene_2006-1,
	title = {The {Gene} {Controlling} the {Quantitative} {Trait} {Locus} \textit{{EPITHIOSPECIFIER} {MODIFIER1}} {Alters} {Glucosinolate} {Hydrolysis} and {Insect} {Resistance} in \textit{{Arabidopsis}}},
