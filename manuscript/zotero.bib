	doi = {10.1186/1471-2229-14-119},
	abstract = {Herbivory imposes an important selective pressure on plants. In Arabidopsis thaliana leaf trichomes provide a key defense against insect herbivory; however, trichome production incurs a fitness cost in the absence of herbivory. Previous work on A. thaliana has shown an increase in trichome density in response to leaf damage, suggesting a mechanism by which the cost associated with constitutively high trichome density might be mitigated; however, the genetic basis of trichome density induction has not been studied.},
	language = {en},
	number = {1},
	urldate = {2015-10-09},
	journal = {BMC Plant Biology},
	author = {Bloomer, Rebecca H. and Lloyd, Alan M. and Symonds, V. V.},
	year = {2014},
	pmid = {24885520},
	note = {Number: 1},
	keywords = {genetic architecture, QTL, plant defense, natural variation, trichome density},
	pages = {119},
}

