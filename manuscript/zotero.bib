}

@article{mandadi_bt2_2009,
	title = {{BT2}, a {BTB} {Protein}, {Mediates} {Multiple} {Responses} to {Nutrients}, {Stresses}, and {Hormones} in {Arabidopsis}},
	volume = {150},
	copyright = {https://creativecommons.org/licenses/by/4.0/},
	issn = {1532-2548},
	url = {https://academic.oup.com/plphys/article/150/4/1930/6108036},
	doi = {10.1104/pp.109.139220},
	abstract = {Abstract
            The Arabidopsis (Arabidopsis thaliana) gene BT2 encodes a 41-kD protein that possesses an amino-terminal BTB domain, a central TAZ domain, and a carboxyl-terminal calmodulin-binding domain. We previously demonstrated that BT2 could activate telomerase expression in mature Arabidopsis leaves. Here, we report its distinct role in mediating diverse hormone, stress, and metabolic responses. We serendipitously discovered that steady-state expression of BT2 mRNA was regulated diurnally and controlled by the circadian clock, with maximum expression in the dark. This pattern of expression suggested that BT2 mRNA could be linked to the availability of photosynthate in the plant. Exogenous sugars decreased BT2 expression, whereas exogenous nitrogen increased expression. bt2 loss-of-function mutants displayed a hypersensitive response to both sugar-mediated inhibition of germination and abscisic acid (ABA)-mediated inhibition of germination, thus supporting a role of ABA in sugar signaling in germination and development. Moreover, constitutive expression of BT2 imparted resistance to both sugars and ABA at germination, suggesting that BT2 suppresses sugar and ABA responses. In support of the previously described antagonistic relationship between ABA and auxin, we found that BT2 positively regulated certain auxin responses in plants, as revealed by knocking down BT2 expression in the high-auxin mutant yucca. Accumulation of BT2 mRNA was affected by a variety of hormones, nutrients, and stresses, and BT2 was required for responses to many of these same factors. Together, these results suggest that BT2 is a central component of an interconnected signaling network that detects and responds to multiple inputs.},
	language = {en},
	number = {4},
	urldate = {2024-05-18},
	journal = {Plant Physiology},
	author = {Mandadi, Kranthi K. and Misra, Anjali and Ren, Shuxin and McKnight, Thomas D.},
	month = aug,
	year = {2009},
	pages = {1930--1939},
	file = {Full Text:/Users/yasuhirosato/Zotero/storage/F9RQYVXZ/Mandadi et al. - 2009 - BT2, a BTB Protein, Mediates Multiple Responses to.pdf:application/pdf},
}

@article{delay_cep3_2019,
	title = {{CEP3} levels affect starvation-related growth responses of the primary root},
	volume = {70},
	copyright = {http://creativecommons.org/licenses/by-nc/4.0/},
	issn = {0022-0957, 1460-2431},
	url = {https://academic.oup.com/jxb/article/70/18/4763/5512447},
	doi = {10.1093/jxb/erz270},
	abstract = {Abstract
            CEPs (C-TERMINALLY ENCODED PEPTIDEs) inhibit Arabidopsis primary root growth by unknown mechanisms. We investigated how CEP3 levels control primary root growth. CEP3 peptide application decreased cell division, S-phase cell number, root meristematic cell number, and meristem zone (MZ) size in a dose- and CEP RECEPTOR1-dependent manner. Grafting showed that CEP3-dependent growth inhibition requires root and shoot CEPR1. CEP3 induced mitotic quiescence in MZ cells significantly faster than that induced by nutrient limitation alone. CEP3 also inhibited the restoration of S-phase to mitotically quiescence cells by nutrient resupply without quantitatively reducing TARGET OF RAPAMYCIN (TOR) kinase activity. In contrast, cep3-1 had an increased meristem size and S-phase cell number under nitrogen (N)-limited conditions, but not under N-sufficient conditions. Furthermore, cep3-1 meristematic cells remained in S-phase longer than wild-type cells during a sustained carbon (C) and N limitation. RNA sequencing showed that CEP3 peptide down-regulated genes involved in S-phase entry, cell wall and ribosome biogenesis, DNA replication, and meristem expansion, and up-regulated genes involved in catabolic processes and proteins and peptides that negatively control meristem expansion and root growth. Many of these genes were reciprocally regulated in cep3-1. The results suggest that raising CEP3 induces starvation-related responses that curtail primary root growth under severe nutrient limitation.},
	language = {en},
	number = {18},
	urldate = {2024-05-18},
	journal = {Journal of Experimental Botany},
	author = {Delay, Christina and Chapman, Kelly and Taleski, Michael and Wang, Yaowei and Tyagi, Sonika and Xiong, Yan and Imin, Nijat and Djordjevic, Michael A},
