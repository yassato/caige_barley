	month = jul,
	year = {2022},
	pages = {e2201285119},
	file = {Brachi et al. - 2022 - Plant genetic effects on microbial hubs impact hos.pdf:/Users/yasuhirosato/Zotero/storage/XY9Z9SL3/Brachi et al. - 2022 - Plant genetic effects on microbial hubs impact hos.pdf:application/pdf},
